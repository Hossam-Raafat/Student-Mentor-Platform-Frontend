%div.col-lg-10.col-lg-offset-1{"ng-controller" => "mentorDashController"}
  %h1 Welcome {{user.name}}!
  %button.btn-status.btn.m-b-xs.btn-sm.btn-primary.btn-addon
    %input#toggle-1{"ng-change" => "editStatus(mentor.status, mentor.id)", "ng-click" => 'class()', "ng-model" => "mentor.status", :type => "checkbox"}
    %span{"ng-if" => "mentor.status == true", "ng-show" => "mentor.status"} online
    %span{"ng-if" => "mentor.status == false", "ng-hide" => "mentor.status" } offline

