%div{"ng-controller" => "managerMentorProfile"}
  %ul
    %li{"ng-repeat" => "question in mentorResolved"}
      %p
        %span> Question:
        {{$index + 1}}
      %p
        Status:
        %span{"ng-if" => "question.response.status == true"} Answered
        %span{"ng-if" => "question.response.status == false"} Not Answered
      %p
        %span Submited
        %time-ago{:format => "MM/dd/yyyy", "from-time" => "{{ question.created_at}}"}
      %p Claimed {{question.response.created_at | amDifference : question.created_at : 'minutes' }} minutes later
      %p
        %span> Title:
        {{question.title}}
      %p
        %span> Body:
        {{question.body}}
      %div{"hm-dots-class" => "dots", "hm-less-text" => "read less", "hm-limit" => "10", "hm-link-class" => "links", "hm-more-text" => "read more", "hm-read-more" => "", "hm-text" => "{{question.response.answer}}"}
%a{"ui-sref" => "managerLayout.managerMentorsList"} Mentors
