%div.col-lg-12{"ng-controller" => "managerStudentProfile"}
  .panel{"ng-repeat" => "question in studentQuestions"}
    .wrapper-lg
      %h2.m-t-none
        %h2 Q{{$index + 1}}: 
        %h4 {{question.title}}
      %div
        %p {{question.body}}
          %h4{"ng-if" => "question.response.status == true"} ANSWER:
          %p{"ng-if" => "question.response.status == true", "hm-dots-class" => "dots", "hm-less-text" => "read less", "hm-limit" => "100", "hm-link-class" => "links", "hm-more-text" => "read more", "hm-read-more" => "", "hm-text" => "{{question.response.answer}}"}
      .line.line-lg.b-b.b-light
      .text-muted
        
        %span Submited 
        %i.icon-clock
          %time-ago{:format => "MM/dd/yyyy", "from-time" => "{{ question.created_at}}"}
        by
        %i.icon-fire
        %span {{question.student.name}}
        %span , Claimed {{question.response.created_at | amDifference : question.created_at : 'minutes' }} minutes later.
        %p Status: 
          %span{"ng-if" => "question.response.status == true"} Answered
            %span With a rate of {{question.rate.star}}/5
          %span{"ng-if" => "question.response.status == false"} Not Answered yet
        
  %a{"ui-sref" => "managerLayout.managerMentorsList"} Mentors    